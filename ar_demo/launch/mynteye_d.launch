<launch>
    <node name="vins_estimator"
          pkg="vins"
          type="vins_d_node"
          output="screen" >
      <param name="config_file"
             type="string"
             value="$(find vins)/../config/mynteye-d/mynt_stereo_imu_config.yaml" />
    </node>
    <node pkg="ar_demo"
          type="ar_demo_node"
          name="ar_demo_node"
          output="screen">
        
        <remap from="~image_raw"
               to="/mynteye/left/image_mono"/>
        <remap from="~camera_pose"
               to="/vins_estimator/camera_pose"/>
        <!-- <remap from="~camera_pose"
               to="/vins_estimator/latest_camera_pose"/> -->
        <remap from="~pointcloud"
               to="/vins_estimator/point_cloud"/>

        <param name="calib_file"
               type="string"
               value="$(find ar_demo)/../config/mynteye-d/device_params_left.yaml"/>
        <param name="use_undistored_img"
               type="bool"
               value="false"/>
    </node>
    <!--  -->
    <!-- <node name="rvizvisualisation"
          pkg="rviz"
          type="rviz"
          output="log"
          args="-d $(find ar_demo)/../config/AR_demo.rviz" /> -->
</launch>